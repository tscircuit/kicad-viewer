{
	"name": "kicad-viewer",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"start": "bun run dev",
		"dev": "vite",
		"build": "tsc && vite build",
		"vercel-build": "bun run build",
		"lint": "biome lint",
		"format": "biome format . --write",
