{
	"buildCommand": "bun run build",
	"outputDirectory": "dist",
	"devCommand": "bun run dev",
	"installCommand": "bun install",
	"framework": "vite",
	"rewrites": [
		{
			"source": "/api/:path*",
			"destination": "https://kicad-mod-cache.tscircuit.com/:path*"
		},
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	]
}
